define StringIterator
(
    current: pointer[char]
    last: pointer[char]
)
{

init(string: String) {
    if string.get_length() > 0
    {
        let buffer string.get_buffer()
        set current: buffer
        set last: buffer + string.get_length()
    }
}

function next(this: StringIterator) returns pointer[char]$ {
    if current = last
    {
        return null
    } else {
        let ret current
        set current: current + 1
        return ret
    }
}

}
