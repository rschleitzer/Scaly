define PageListIterator(current: pointer[PageNode])
{
    init(head: pointer[PageNode] head)
        set this.current: head

    procedure next(this: PageListIterator) returns PageNode?
    {
        if this.current <> null
        {
            let current this.current
            set this.current: this.current*.next
            return option current.element
        }
        else
        {
            option[PageNode] null
        }
    }
}
