        choose parse_local#()
            when error: ParserError
            {
                choose error
                    when i: Invalid
                        throw ParserError.Invalid(i)
                    when d: Different {}
            }
            when node: LocalSyntax
                return LifetimeSyntax.Local(node)
        choose parse_reference#()
            when error: ParserError
            {
                choose error
                    when i: Invalid
                        throw ParserError.Invalid(i)
                    when d: Different {}
            }
            when node: ReferenceSyntax
                return LifetimeSyntax.Reference(node)
        choose parse_thrown#()
            when error: ParserError
            {
                choose error
                    when i: Invalid
                        throw ParserError.Invalid(i)
                    when d: Different {}
            }
            when node: ThrownSyntax
                return LifetimeSyntax.Thrown(node)
        throw ParserError.Different(DifferentSyntax())
    }

