define BuilderListIterator[T] (current: pointer[Node[T]]) {
    function next(this: BuilderListIterator[T]) returns pointer[T] {
        if current <> null
        {
            let next_node current
            set current: (*current).next
            return &(*next_node).element
        }
        else
        {
            return null
        }
    }
}
