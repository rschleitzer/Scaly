use LLVM.*

define Double (double) {}
define Long (i64) {}
define Integer (i32) {}
define Byte (i8) {}
define Boolean (i1) {}

define Size (i64)
{
    operator 'as Integer' returns Integer
        LLVM.trunc[i64, i32](this)
}

define Pointer[T](this: i64)
{
    operator * returns T
        LLVM.load[T](this)
}